    426              if (plist->proc)
    427              {
    428                proc_ret=plist->proc(submsg,msg);
    429                switch(proc_ret)
    430                {
    431                case KEYHOOK_NEXT:
    432                  break;
    433                case KEYHOOK_NEXT_NOT2GUI:
    434                  is_no_gui=1;
    435                  break;
    436                case KEYHOOK_BREAK:
    437                  return (2);
    438                case KEYHOOK_ENDQ:
    439                  return is_no_gui==1?2:0;
    440                default:
    441                  break;
    442                }      
    443              }
    444              plist=plist->next;
    445            }
    446            return is_no_gui;
    447          }
