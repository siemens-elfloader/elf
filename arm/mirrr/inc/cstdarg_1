// cstdarg standard header
#ifndef _CSTDARG_
#define _CSTDARG_

#ifndef _SYSTEM_BUILD
  #pragma system_include
#endif

#include <yvals.h>

#ifdef _STD_USING
  #undef _STD_USING
  #include <stdarg.h>
  #define _STD_USING
#else /* _STD_USING */
  #include <stdarg.h>

  #ifdef _GLOBAL_USING
    _STD_BEGIN
    using _CSTD va_list;
    _STD_END
  #endif /* _GLOBAL_USING */
#endif /* _STD_USING */
#endif /* _CSTDARG_ */

