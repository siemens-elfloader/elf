// cerrno standard header
#ifndef _CERRNO_
#define _CERRNO_

#ifndef _SYSTEM_BUILD
  #pragma system_include
#endif

#include <yvals.h>

#ifdef _STD_USING
  #undef _STD_USING
  #include <errno.h>
  #define _STD_USING
#else /* _STD_USING */
  #include <errno.h>

  #ifdef _GLOBAL_USING
    _STD_BEGIN
    #ifndef errno
      // using std::errno;
    #endif
    _STD_END
  #endif /* _GLOBAL_USING */
#endif /* _CERRNO_ */
#endif /* _STD_USING */
