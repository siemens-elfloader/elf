    648          static int OnKey(MAIN_GUI *data, GUI_MSG *msg)//Ну это кей хук
    649          {
    650            //char box[128];
    651            if (msg->gbsmsg->msg==KEY_DOWN)//Если клавиша нажата
    652            {
    653               if (typeGUI==0)  // карта
    654               {
    655                 switch(msg->gbsmsg->submess)
    656                 { 
    657                   case UP_BUTTON: case '2': // Клавиша вверх
    658                   {
    659                     if(cursorY-scrY==0 && scrY>0) scrY--;
    660                     if(cursorY!=0) cursorY--;
    661                     return 0;
    662                   }
    663                   case DOWN_BUTTON: case '8': // Клавиша вниз
    664                   {
    665                     if(cursorY-scrY==sizeY-1 && scrY<mapy-sizeY) scrY++;
    666                     if(cursorY<mapy-1) cursorY++;
    667                     return 0;
    668                   }
    669                   case RIGHT_BUTTON: case '6': // Клавиша вправо
    670                   {
    671                     if(cursorX-scrX==sizeX-1 && scrX<mapx-sizeX) scrX++;
    672                     if(cursorX<mapx-1) cursorX++;
    673                     return 0;
    674                   }
    675                   case LEFT_BUTTON: case '4': // Клавиша влево
    676                   {
    677                     if(cursorX-scrX==0 && scrX>0) scrX--;
    678                     if(cursorX!=0) cursorX--;
    679                     return 0;
    680                   }           
    681                   case '0':
    682                   {
    683                     typeGUI=3;
    684                     return 0;
    685                   }     
    686                   case ENTER_BUTTON: // Клавиша выбор
    687                   {
    688                     typeGUI=2;
    689                     return 0;
    690                   }
    691                   case LEFT_SOFT: // Левый софт(меню)
    692                   {
    693                     typeGUI=1;
    694                     return 0;
    695                   }
    696                   case RIGHT_SOFT:case RED_BUTTON: return (1); //Выход
    697                 }
    698               }
    699               if (typeGUI==1)  //  меню
    700               {
    701                 switch(msg->gbsmsg->submess)
    702                 {
    703                   case UP_BUTTON: case '2': // Вверх
    704                   {
    705                     countM2 = (countM2 == 1 ? counCOLM : countM2-1);
    706                     return 0;
    707                   }
    708                   case DOWN_BUTTON: case '8': // Вниз
    709                   {
    710                     countM2 = (countM2 == counCOLM ? 1 : countM2+1);
    711                     return 0;
    712                   }
    713                   case LEFT_SOFT: // Левый софт(меню)
    714                   {
    715                     typeGUI=0;
    716                     return 0;
    717                   }
    718                   case ENTER_BUTTON:
    719                   {
    720                     MenuEnter();
    721                     if(countM2==8) return 1;
    722                     typeGUI = 0;
    723                     return 0;
    724                    }
    725                    case RIGHT_SOFT: typeGUI=2; REDRAW (); return 0; // Правый софт
    726                    case RED_BUTTON: return (1); // Выход
    727                  }
    728                }
    729               if (typeGUI==2)  // замена текущей картинки в карте
    730               {
    731                 switch(msg->gbsmsg->submess)
    732                 {
    733                   case RIGHT_BUTTON: case '6': // Вправо
    734                   { 
    735                     if (tpic<maxpic) tpic++;
    736                     printPCUR ();
    737                     return 0;
    738                   }
    739                   case LEFT_BUTTON: case '4': // Влево
    740                   {
    741                     if (tpic > minpic) tpic--;
    742                     printPCUR ();
    743                     return 0;
    744                   }
    745                   
    746                   case '#':
    747                   { 
    748                     tpic = (tpic+10)%maxpic;
    749                     printPCUR ();
    750                     return 0;
    751                   }
    752                   case '*':
    753                   {
    754                     tpic = (tpic+maxpic-10)%maxpic;
    755                     printPCUR ();
    756                     return 0;
    757                   }
    758                   
    759                   case ENTER_BUTTON: // Выбор
    760                   { 
    761                     int i;
    762                     i=npic2save ();
    763                     map[i]=tpic;
    764                     REDRAW();
    765                     typeGUI=0;
    766                     return 0;
    767                   }
    768                   
    769                   case '1': // Выбор
    770                   { 
    771                     int i;
    772                     i=npic2save ();
    773                     map[i]=1;
    774                     REDRAW();
    775                     return 0;
    776                   }
    777                   case LEFT_SOFT: typeGUI=1; return 0;
    778                   case RIGHT_SOFT: case RED_BUTTON: return (1);
    779                 }
    780               }
    781               
    782               if (typeGUI==3)  //миникарта
