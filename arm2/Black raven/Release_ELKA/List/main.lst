    136          void toInt32(int in, char *cint32)
    137          {
    138            if (in <= 255) 
    139              {
    140                cint32[0] = 0x00;
    141                cint32[1] = 0x00;
    142                cint32[2] = 0x00;
    143                cint32[3] = in;
    144              }
    145              else if(in <= 32767)
    146              {
    147                cint32[0] = 0x00;
    148                cint32[1] = 0x00;
    149                cint32[2] = in / 128;
    150                cint32[3] = in % 128;
    151              }
    152              else
    153              {
    154                int tm = 0;
    155                cint32[0] = 0x00;
    156                tm = in - (in % 16384);
    157                cint32[1] = tm / 16384;
    158                tm = in % 16384;
    159                cint32[2] = tm / 128;
    160                cint32[3] = tm % 128;
    161              }
    162          }
