    129          int getInt32(char* dat, int ps)
    130          {
    131            int nm = 0;
    132            nm = dat[ps + 0] * 0x200000 + dat[ps + 1] * 0x4000 + dat[ps + 2] * 0x80 + dat[ps + 3];
    133            return nm;
    134          }
